@PACKAGE_INIT@

SET(PHIGS_VERSION @PHIGS_VERSION@)
SET(PHIGS_VERSION_MAJOR  @PHIGS_VERSION_MAJOR@)
SET(PHIGS_VERSION_MINOR  @PHIGS_VERSION_MINOR@)
SET(PHIGS_VERSION_PATCH  @PHIGS_VERSION_PATCH@)


set_and_check(PHIGS_INCLUDE_DIR @PACKAGE_CMAKE_INSTALL_INCLUDEDIR@)

find_library(PHIGS_LIB NAMES phigs HINTS @PACKAGE_CMAKE_INSTALL_LIBDIR@)
find_library(PHIGS_VK_LIB NAMES Vk HINTS @PACKAGE_CMAKE_INSTALL_LIBDIR@)
find_library(PHIGS_GLW_LIB NAMES GLw HINTS @PACKAGE_PHIGS_ROOTIO_INSTALL_LIBDIR@)

set(PHIGS_LIBRARIES ${PHIGS_LIB} ${PHIGS_VK_LIB} PHIGS_GLW_LIB)
